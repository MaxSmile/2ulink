{
  "hosting": {
    "site": "2ulink",
    "name": "2ulink",
    "cleanUrls": true,
    "project": "ul-top",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "apiProxy"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
